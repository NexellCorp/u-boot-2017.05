			      "gpioa-12", "gpiod-0", "gpiod-1", "gpiod-2";
		nexell,pin-function = <NX_PIN_FUNC2>;
		nexell,pin-pull = <NX_PIN_PULL_NONE>;
		nexell,pin-strength = <NX_PIN_STR0>;
	};

	dp_red: dp-red {
